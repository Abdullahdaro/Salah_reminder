{
  "name": "Salah Reminder",
  "description": "Get a notifications for Salah time and dont spend your day without praying at time",
  "version": "1.1",
  "manifest_version": 3,
  "permissions": ["storage", "alarms", "notifications"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "Salah.html",
    "default_icon": "Salah.png"
  },
  "icons": {
    "128": "Salah.png"
  },
  "host_permissions": ["https://api.aladhan.com/*"]
}
